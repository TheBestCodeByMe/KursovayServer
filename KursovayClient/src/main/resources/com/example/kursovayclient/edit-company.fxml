<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.admin.EditCompany">
<children>
    <Button fx:id="buttonBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickBack" text="Назад" />
    <TableView fx:id="tableCompany" layoutX="38.0" layoutY="72.0" prefHeight="256.0" prefWidth="393.0">
        <columns>
            <TableColumn fx:id="columnId" prefWidth="75.0" text="Id" />
            <TableColumn fx:id="columnName" prefWidth="155.20001220703125" text="Company's name" />
            <TableColumn fx:id="columnNumbEmpl" prefWidth="162.39996337890625" text="Number of employees" />
        </columns>
    </TableView>
      <Button fx:id="butttonAdd" layoutX="445.0" layoutY="174.0" mnemonicParsing="false" onAction="#clickAdd" text="Добавить" />
      <Button fx:id="buttonUpdate" layoutX="484.0" layoutY="221.0" mnemonicParsing="false" onAction="#clickUpdate" text="Обновить" />
      <Button fx:id="buttonDelete" layoutX="533.0" layoutY="174.0" mnemonicParsing="false" onAction="#clickDelete" text="Удалить" />
      <TextField fx:id="txtName" layoutX="444.0" layoutY="122.0" promptText="Введите имя компании" />
      <Button fx:id="buttonEditName" layoutX="442.0" layoutY="271.0" mnemonicParsing="false" onAction="#clickEditName" text="Изменить имя компании" />
</children>
</AnchorPane>
